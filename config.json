{
	"type": "minecraftBedrock",
	"name": "Underrated Anime Magic Pack",
	"namespace": "uam",
	"authors": [
		"pyppiix"
	],
	"targetVersion": "1.21.130",
	"description": "This Addon is have 5 Underrated Anime for Bedrock version",
	"experimentalGameplay": {
		"upcomingCreatorFeatures": true,
		"experimentalCreatorCameraFeatures": false,
		"enableGameTestFramework": true,
		"renderDragonFeatures": false,
		"educationEdition": false
	},
	"bdsProject": true,
	"packs": {
		"behaviorPack": "./BP",
		"resourcePack": "./RP"
	},
	"worlds": [
