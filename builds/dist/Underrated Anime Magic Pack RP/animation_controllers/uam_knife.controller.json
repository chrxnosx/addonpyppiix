{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.uam.kaname_knife": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"sword_slash": "query.is_using_item&&query.get_equipped_item_name=='uam:darwin_kaname_knife'"
						}
					]
				},
				"sword_slash": {
					"on_entry": [
						"/event entity @s uam:sword_slash"
					],
					"transitions": [
						{
							"default": "!query.is_using_item"
						}
					]
				}
			}
		}
	}
}